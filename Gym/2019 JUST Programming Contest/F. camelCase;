#F. camelCase;

for tt in range(int(input())):
    s = input()
 
    if len(s) == 0 :
        print('YES')
 
    else:
        cnt = 1
        for i in range(len(s)):
            if s[i].isupper() :
                cnt += 1
 
        #print(cnt)
        if cnt <= 7 :
            print('YES')
 
        else:
            print('NO')
